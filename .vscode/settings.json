{
    "actionButtons": {
        "defaultColor": "coral",
        "loadNpmCommands": false,
        "commands": [
            {
                "name": "$(terminal) Run Dev",
                "singleInstance": true,
                "command": "npm run dev"
            },
            {
                "name": "$(arrow-up) Push Prisma Schema",
                "singleInstance": true,
                "command": "npx prisma db push"
            },
            {
                "name": "$(inspect) Prisma Studio",
                "singleInstance": true,
                "command": "npx prisma studio"
            }
        ]
    }
}
